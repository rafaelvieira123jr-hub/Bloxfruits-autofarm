-- ============================================
-- FUN√á√ÉO EQUIPAR ARMA (Optimizada)
-- ============================================
function EquipWeapon(ToolSe)
    local backpack = game.Players.LocalPlayer.Backpack
    local character = game.Players.LocalPlayer.Character
    if not character then return end
    
    if ToolSe == "Melee" then
        for _, v in pairs(backpack:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Combat" then
                character.Humanoid:EquipTool(v)
                break
            end
        end
    elseif ToolSe == "Sword" then
        for _, v in pairs(backpack:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Sword" then
                character.Humanoid:EquipTool(v)
                break
            end
        end
    elseif ToolSe == "Blox Fruit" then
        for _, v in pairs(backpack:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Blox Fruit" then
                character.Humanoid:EquipTool(v)
                break
            end
        end
    end
end

-- ============================================
-- FUN√á√ÉO CHECKQUEST COMPLETA (Optimizada)
-- ============================================
function CheckQuest()
    local MyLevel = game.Players.LocalPlayer.Data.Level.Value
    
    -- [Todas as condi√ß√µes de level permanecem iguais]
    -- Manter todo o c√≥digo da fun√ß√£o CheckQuest() original
    -- [C√≥digo completo da CheckQuest() aqui...]
    
    -- Exemplo de estrutura (mantenha todo o c√≥digo original):
    if World1 then
        if MyLevel == 1 or MyLevel <= 9 then
            Mon = "Bandit"
            LevelQuest = 1
            NameQuest = "BanditQuest1"
            NameMon = "Bandit"
            CFrameQuest = CFrame.new(1059.37195, 15.4495068, 1550.4231)
            CFrameMon = CFrame.new(1045.962646484375, 27.00250816345215, 1560.8203125)
        -- [...] continuar com todas as condi√ß√µes
        end
    elseif World2 then
        -- [...] condi√ß√µes World2
    elseif World3 then
        -- [...] condi√ß√µes World3
    end
end

-- ============================================
-- LOOP PRINCIPAL DE AUTO FARM (Optimizado)
-- ============================================
task.spawn(function()
    while task.wait(0.1) do
        if _G.AutoFarmLevel then
            pcall(function()
                CheckQuest()
                local player = game.Players.LocalPlayer
                local questGUI = player.PlayerGui.Main.Quest
                
                if not string.find(questGUI.Container.QuestTitle.Title.Text, NameMon) then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                end
                
                if not questGUI.Visible then
                    local hrp = player.Character.HumanoidRootPart
                    hrp.CFrame = CFrameQuest
                    
                    if (CFrameQuest.Position - hrp.Position).Magnitude <= 50 then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, LevelQuest)
                        task.wait(0.5)
                    end
                elseif questGUI.Visible then
                    local enemies = game:GetService("Workspace").Enemies
                    
                    if enemies:FindFirstChild(Mon) then
                        for _, v in pairs(enemies:GetChildren()) do
                            if v.Name == Mon and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                if string.find(questGUI.Container.QuestTitle.Title.Text, NameMon) then
                                    EquipWeapon(SelectedWeapon)
                                    v.HumanoidRootPart.CanCollide = false
                                    v.Humanoid.WalkSpeed = 0
                                    v.HumanoidRootPart.Size = Vector3.new(70,70,70)
                                    
                                    local hrp = player.Character.HumanoidRootPart
                                    hrp.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
                                    
                                    if _G.FastAttack then
                                        -- Adicionar sistema de fast attack aqui
                                        game:GetService("VirtualInputManager"):SendKeyEvent(true, "X", false, nil)
                                        game:GetService("VirtualInputManager"):SendKeyEvent(false, "X", false, nil)
                                    end
                                end
                                break
                            end
                        end
                    else
                        player.Character.HumanoidRootPart.CFrame = CFrameMon
                    end
                end
            end)
        end
        
        -- Anti AFK
        if _G.AntiAFK then
            local vu = game:GetService("VirtualUser")
            vu:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
            task.wait(1)
            vu:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
        end
        
        -- Auto Stats
        if _G.AutoStats then
            pcall(function()
                local statType = _G.SelectedStats
                if statType == "Blox Fruit" then statType = "Demon Fruit" end
                
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", statType, 1)
            end)
        end
    end
end)

-- ============================================
-- FUN√á√ïES ADICIONAIS PARA DELTA EXECUTOR
-- ============================================

-- Sistema de Infinite Jump otimizado
if _G.InfiniteJump then
    game:GetService("UserInputService").JumpRequest:Connect(function()
        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
        end
    end)
end

-- Sistema de NoClip otimizado
task.spawn(function()
    while task.wait(0.1) do
        if _G.NoClip and game.Players.LocalPlayer.Character then
            for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA("BasePart") and v.CanCollide then
                    v.CanCollide = false
                end
            end
        end
    end
end)

-- Sistema de Speed Boost otimizado
task.spawn(function()
    while task.wait(0.1) do
        if _G.SpeedBoost and game.Players.LocalPlayer.Character then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
        elseif game.Players.LocalPlayer.Character then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    end
end)

-- ============================================
-- REMOVER TEXTOS DE DANO
-- ============================================
task.spawn(function()
    game:GetService('RunService').Stepped:Connect(function()
        if _G.Remove_Effect then
            for _, v in pairs(game:GetService("ReplicatedStorage").Effect.Container:GetChildren()) do
                if v.Name == "Death" then
                    v:Destroy() 
                end
            end
        end
    end)
end)

-- ============================================
-- INICIALIZAR GUI
-- ============================================
OrionLib:Init()

